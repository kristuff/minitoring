[
    {
        "name": "Apache access",
        "type": "apache_access",
        "software":"apache",
        "paths": [
            "/var/log/" ,
            "/var/log/apache/" ,
            "/var/log/apache2/" ,
            "/var/log/httpd/" ,
            "/usr/local/var/log/apache/" ,
            "/usr/local/var/log/apache2/" ,
            "/usr/local/var/log/httpd/" ,
            "/opt/local/apache/logs/" ,
            "/opt/local/apache2/logs/" ,
            "/opt/local/httpd/logs/" ,
            "C:/wamp/logs/"
        ],
        "default_file": "/var/log/apache2/access.log",
        "files": [
            "access.log" ,
            "access_log" ,
            "apache.log" ,
            "apache_access.log" 
        ],
        "columns": [
            {
                "code":"%t",
                "name":"time",
                "display":"Date"
            },
            {
                "code":"%v",
                "name":"serverName",
                "display":"Server Name"
            },
            {
                "code":"%V",
                "name":"canonicalServerName",
                "display":"Canonical Server Name"
            },
            {
                "code":"%p",
                "name":"port",
                "display":"Port"
            },
            {
                "code":"%h",
                "name":"host",
                "display":"IP"
            },
            {
                "code":"%a",
                "name":"remoteIp",
                "display":"Remote IP"
            },
            {
                "code":"%A",
                "name":"localIp",
                "display":"Local IP"
            },
            {
                "code":"%u",
                "name":"user",
                "display":"User"
            },
            {
                "code":"%l",
                "name":"logname",
                "display":"Log Name"
            },
            {
                "code":"%m",
                "name":"requestMethod",
                "display":"Method"
            },
            {
                "code":"%U",
                "name":"URL",
                "display":"URL"
            },
            {
                "code":"%H",
                "name":"requestProtocol",
                "display":"Protocol"
            },
            {
                "code":"%r",
                "name":"request",
                "display":"Request"
            },
            {
                "code":"%>s",
                "name":"status",
                "display":"Status"
            },
            {
                "code":"%s",
                "name":"status",
                "display":"Status"
            },
            {
                "code":"%O",
                "name":"sentBytes",
                "display":"Size"
            },
            {
                "code":"%b",
                "name":"responseBytes",
                "display":"Response Size"
            },
            {
                "code":"%T",
                "name":"requestTime",
                "display":"Request Time"
            },
            {
                "code":"%D",
                "name":"timeServeRequest",
                "display":"Time Server Request"
            },
            {
                "code":"%I",
                "name":"receivedBytes",
                "display":"Received Bytes"
            },
            {
                "code":"%{Referer}i",
                "name":"headerReferer",
                "display":"Referer"
            },
            {
                "code":"%{User-Agent}i",
                "name":"headerUserAgent",
                "display":"User Agent"
            }
        ],
        "formats": [
            {
                "name": "common",
                "format": "%h %l %u %t \"%r\" %>s %O"
            },
            {
                "name": "vhost_common",
                "format":"%v %h %l %u %t \"%r\" %>s %O"
            },
            {
                "name": "combined",
                "format": "%h %l %u %t \"%r\" %>s %O \"%{Referer}i\" \"%{User-Agent}i\""
            },
            {
                "name": "vhost_combined",
                "format": "%v:%p %h %l %u %t \"%r\" %>s %O \"%{Referer}i\" \"%{User-Agent}i\""
            },
            {
                "name": "referer",
                "format": "%{Referer}i"
            },
            {
                "name": "agent",
                "format": "%{User-Agent}i"
            },
            {
                "name": "custom",
                "format": ""
            }
        ]
    },
    {
        "name": "Apache error",
        "type": "apache_error",
        "paths": [  
            "/var/log/" ,
            "/var/log/apache/" ,
            "/var/log/apache2/" ,
            "/var/log/httpd/" ,
            "/usr/local/var/log/apache/" ,
            "/usr/local/var/log/apache2/" ,
            "/usr/local/var/log/httpd/" ,
            "/opt/local/apache/logs/" ,
            "/opt/local/apache2/logs/" ,
            "/opt/local/httpd/logs/" ,
            "C:/wamp/logs/"
        ],
        "files": [
            "error.log" ,
            "error_log" ,
            "apache_error.log" 
        ],
        "default_file": "/var/log/apache2/error.log",
        "columns": [
            {
                "code":"%t",
                "name":"time",
                "display":"Date"
            },
            {
                "code":"%{u}t",
                "name":"time",
                "display":"Date"
            },
            {
                "code":"%a",
                "name":"remoteIp",
                "display":"Remote IP"
            },
            {
                "code":"%A",
                "name":"localIp",
                "display":"Local IP"
            },
            {
                "code":"%-m",
                "name":"module",
                "display":"Module"
            },
            {
                "code":"%m",
                "name":"module",
                "display":"Module"
            },
            {
                "code":"%l",
                "name":"level",
                "display":"Level"
            },
            {
                "code":"%P",
                "name":"pid",
                "display":"PID"
            },
            {
                "code":"%T",
                "name":"tid",
                "display":"TID"
            },
            {
                "code":"%E:",
                "name":"errorCode",
                "display":"ErrorCode"
            },
            {
                "code":"%F:",
                "name":"fileName",
                "display":"FileName"
            },
            {
                "code":"%M",
                "name":"message",
                "display":"Message"
            },
            {
                "code":"referer",
                "name":"referer",
                "display":"Referer"
            }
        ],
        "formats": [
            {
                "name": "apache2.2 default",
                "format": "[%t] [%l] %E: [client %a] %M"
            }, 
            {
                "name": "apache2.2 extented",
                "format": "[%t] [%l] %F: %E: [client %a] %M"
            }, 
            {
                "name": "apache2.2 referer",
                "format": "[%t] [%l] %E: [client %a] %M , referer %{Referer}i"
            }, 
            {
                "name": "apache2.2 extended referer",
                "format": "[%t] [%l] %F: %E: [client %a] %M , referer %{Referer}i"
            }, 
            {
                "name": "apache2.4 default",
                "format": "[%{u}t] [%l] [pid %P] %E: [client %a] %M"
            }, 
            {
                "name": "apache2.4 extended",
                "format": "[%{u}t] [%l] [pid %P] %F: %E: [client %a] %M"
            },
            {
                "name": "apache2.4 referer",
                "format": "[%{u}t] [%l] [pid %P] %E: [client %a] %M , referer %{Referer}i"
            }, 
            {
                "name": "apache2.4 extended referer",
                "format": "[%{u}t] [%l] [pid %P] %F: %E: [client %a] %M , referer %{Referer}i"
            },
            {
                "name": "apache2.4 mpm",
                "format": "[%{u}t] [%-m:%l] [pid %P] %E: [client %a] %M"
            },
            {
                "name": "apache2.4 mpm extented",
                "format": "[%{u}t] [%-m:%l] [pid %P] %F: %E: [client %a] %M"
            },
            {
                "name": "apache2.4 mpm referer",
                "format": "[%{u}t] [%-m:%l] [pid %P] %E: [client %a] %M , referer %{Referer}i"
            },
            {
                "name": "apache2.4 mpm extended referer",
                "format": "[%{u}t] [%-m:%l] [pid %P] %F: %E: [client %a] %M , referer %{Referer}i"
            },
            {
                "name": "apache2.4 mpm tid",
                "format": "[%{u}t] [%-m:%l] [pid %P:tid %T] %F: %E: [client %a] %M"
            },
            {
                "name": "apache2.4 mpm tid referer",
                "format": "[%{u}t] [%-m:%l] [pid %P:tid %T] %F: %E: [client %a] %M , referer %{Referer}i"
            },
            {
                "name": "custom",
                "format": ""
            }
        ]
    },
    {
        "name": "Fail2ban",
        "type": "fail2ban",
        "paths": [  
            "/var/log/"
        ],
        "files": [
            "fail2ban.log" 
        ],
        "default_file": "/var/log/fail2ban.log",
        "columns": [
            {
                "code":"%t",
                "name":"time",
                "display":"Date"
            },
            {
                "code":"%s",
                "name":"service",
                "display":"Service"
            },
            {
                "code":"%p",
                "name":"pid",
                "display":"PID"
            },
            {
                "code":"%l",
                "name":"level",
                "display":"Level"
            },
            {
                "code":"%j",
                "name":"jail",
                "display":"Jail"
            },
            {
                "code":"%m",
                "name":"message",
                "display":"Message"
            }
        ],
        "formats": [
            {
                "name": "default",
                "format": "%t %s %p %l %j %m"
            },
            {
                "name": "custom",
                "format": ""
            }
        ]
    },
    {
        "name": "Syslog",
        "type": "syslog",
        "paths": [  
            "/var/log/"
        ],
        "files": [
            "syslog",
            "messages",
            "auth.log", 
            "daemon.log", 
            "kern.log", 
            "mail.err", 
            "mail.info", 
            "mail.warn" 
        ],
        "default_file": "/var/log/syslog",
        "columns": [
            {
                "code":"%t",
                "name":"time",
                "display":"Date"
            },
            {
                "code":"%h",
                "name":"hostname",
                "display":"Hostname"
            },
            {
                "code":"%s",
                "name":"service",
                "display":"Service"
            },
            {
                "code":"%p",
                "name":"pid",
                "display":"PID"
            },
            {
                "code":"%m",
                "name":"message",
                "display":"Message"
            }
        ],
        "formats": [
            {
                "name": "default",
                "format": "%t %h %s%p: %m"
            },
            {
                "name": "custom",
                "format": ""
            }
            
        ]
    }
    
]